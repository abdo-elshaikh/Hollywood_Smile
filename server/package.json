{
  "name": "web_server",
  "version": "1.0.0",
  "description": "Backend server for a web application using Express.js",
  "main": "server.js",
  "scripts": {
    "install": "npm install",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "format": "npm run prettier && npm run lint:fix"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^6.0.0",
    "express-session": "^1.17.1",
    "express-validator": "^6.10.0",
    "file-type": "^16.0.0",
    "fs-extra": "^11.2.0",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^9.0.2",
    "mime": "^4.0.4",
    "mongoose": "^5.12.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "sanitize-filename": "^1.6.3",
    "socket.io": "^4.0.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.7",
    "supertest": "^7.0.0"
  },
  "keywords": [
    "express",
    "backend",
    "server"
  ],
  "author": "Abdo Mhmd",
  "license": "ISC"
}